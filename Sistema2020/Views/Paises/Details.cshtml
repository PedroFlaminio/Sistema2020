@model Sistema2020.Models.Pais

@{
    ViewData["Title"] = "Detalhes do Estado";
}

<br />
<h2>@ViewData["Title"]</h2>
<hr />

<div>
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Codigo)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Codigo)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Nome)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Nome)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model.Id" class="btn btn-warning active">Alterar</a>
    <a asp-action="Index" class="btn btn-secondary">Cancelar</a>
</div>
